   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def a(k) -> float:\n",
    "    if k==0:\n",
    "        return 1\n",
    "    \n",
    "    akm1 = a(k-1)\n",
    "    \n",
    "    return akm1*(1 + b(k)/akm1 * log(a(k)/akm1))\n",
    "\n",
    "def b(k) -> float:\n",
    "    if k==0:\n",
    "        return 0\n",
    "\n",
    "    return 2*a(k-1) - b(k-1)\n",
    "\n"
   ]
