   "source": [
    "from numpy import arange, pi\n",
    "from matplotlib import pyplot\n",
    "from matplotlib.pyplot import plot"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "a73d4d3c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def delta(r, eps):\n",
    "    A = 3/(pi*eps**3)\n",
    "    return A*(eps-r)*(r<eps)"
   ]
  },
